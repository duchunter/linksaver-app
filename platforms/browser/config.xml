<?xml version='1.0' encoding='utf-8'?>
<widget id="com.gp.linksaver" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Linksaver</name>
    <description>
      Hybrid Vue + Cordova
    </description>
    <author email="duc71098@gmail.com" href="https://github.com/duchunter">
        Gangplank
    </author>
    <content src="dist/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
